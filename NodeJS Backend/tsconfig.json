{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "resolveJsonModule": true,
    "declaration": true,
    "strict": true,
    //"noImplicitAny": true,
    "strictNullChecks": true,
    "outDir": "compiled",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2020.promise"
    ]
  },
  "exclude": [
    "node_modules",
    "compiled"
  ]
}